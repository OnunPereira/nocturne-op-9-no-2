	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation",
				"meta.brace",
			],
			"settings": {
				"foreground": "#597387"
			}
		},
		{
			"scope": [
				"entity",
				"entity.name.function",
				"meta.method",
				"punctuation.definition.entity",
				"support.function",
			],
			"settings": {
				"foreground": "#e9bb8b"
			}
		},
		{
			"scope": [
				"keyword",
				"punctuation.definition.keyword"
			],
			"settings": {
				"foreground": "#CBC8FF"
			}
		},
		{
			"scope": [
				"storage",
				"support.class",
				"support.constant",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#CBC8FF"
			}
		},
		{
			"scope": [
				"constant",
				"keyword.operator.comparison",
				"keyword.operator.logical",
				"keyword.operator.relational",
				"keyword.other",
				"support.type",
			],
			"settings": {
				"foreground": "#FF8788"
			}
		},
		{
			"scope": [
				"string",
				"markup.inline"
			],
			"settings": {
				"foreground": "#B7F5BF"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"entity.name",
				"keyword.other.unit",
				"variable.language",
			],
			"settings": {
				"foreground": "#8DCCFF"
			}
		},
		{
			"scope": [
				"support.type.property-name",
				"support.variable",
				"variable",
				"source.js"
			],
			"settings": {
				"foreground": "#cdd"
			}
		},
		{
			"scope": [],
			"settings": {
				"foreground": "#597387"
			}
		},
		{
			"scope": [
				"variable.other.property",
				"variable.scss"
			],
			"settings": {
				"foreground": "#B0D0DD",
			}
		},
		{
			"name": "bold",
			"scope": [
				"markup.bold",
				"punctuation.definition.bold",
				"entity.other.attribute-name.id"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "italic",
			"scope": [
				"comment",
				"markup.italic",
				"punctuation.definition.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		}
	]
